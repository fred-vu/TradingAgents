# ---------------------------------------------------------------------------
# Core LLM access
# ---------------------------------------------------------------------------
OPENAI_API_KEY=openai_api_key_placeholder
# Optional override for self-hosted proxies
# OPENAI_BASE_URL=https://api.openai.com/v1

# Alternative / supplemental LLM gateways (only set what you use)
OPENROUTER_API_KEY=openrouter_api_key_placeholder
ANTHROPIC_API_KEY=anthropic_api_key_placeholder
GOOGLE_API_KEY=google_api_key_placeholder

# OpenRouter advanced settings
# TRADINGAGENTS_OPENROUTER_SELECTION_MODE can be: balanced, cost_balanced, performance_first, free_only
TRADINGAGENTS_OPENROUTER_SELECTION_MODE=free_only
# Max OpenRouter calls per minute (set higher if you have dedicated quota)
TRADINGAGENTS_OPENROUTER_MAX_CALLS=16

# ---------------------------------------------------------------------------
# Market data vendors
# ---------------------------------------------------------------------------
ALPHA_VANTAGE_API_KEY=alpha_vantage_api_key_placeholder
FINNHUB_API_KEY=finnhub_api_key_placeholder
NEWSAPI_API_KEY=newsapi_key_placeholder
# Optional alternates (uncomment if you enable them in config)
# EODHD_API_KEY=eodhd_key_placeholder
# TUSHARE_API_TOKEN=tushare_token_placeholder

# Vendor routing & circuit breaker tuning
TRADINGAGENTS_VENDOR_PRIORITY_ORDER=alpha_vantage,yfinance
TRADINGAGENTS_VENDOR_CB_THRESHOLD=3
TRADINGAGENTS_VENDOR_CB_COOLDOWN=300

# Cache database (SQLite) and TTLs (seconds)
TRADINGAGENTS_CACHE_DB=./cache/trading_data.db
TRADINGAGENTS_TTL_NEWS=3600
TRADINGAGENTS_TTL_FUNDAMENTALS=86400
# TRADINGAGENTS_TTL_STOCK_DATA=900
# TRADINGAGENTS_TTL_INDICATORS=900

# ---------------------------------------------------------------------------
# Desktop bundle helpers (Tauri)
# ---------------------------------------------------------------------------
# Absolute path to the Python interpreter the packaged app should spawn
# TRADINGAGENTS_PYTHON=C:\\Users\\you\\TradingAgents\\.venv\\Scripts\\python.exe
# TRADINGAGENTS_PYTHON=/Users/you/TradingAgents/.venv/bin/python

# Custom path to run_backend.py when testing custom layouts
# TRADINGAGENTS_BACKEND_SCRIPT=C:\\path\\to\\run_backend.py
# TRADINGAGENTS_BACKEND_SCRIPT=/path/to/run_backend.py

# ---------------------------------------------------------------------------
# Observability (LangSmith) & logging
# ---------------------------------------------------------------------------
TRADINGAGENTS_ENABLE_LANGSMITH=0
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=TradingAgents

# Optional overrides for log/audit/output directories
# TRADINGAGENTS_RESULTS_DIR=./results
# TRADINGAGENTS_LOG_DIR=./logs
# TRADINGAGENTS_AUDIT_LOG_DIR=./audit_logs
# TRADINGAGENTS_CONFIG_DIR=./.tradingagents
