{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "TradingAgents Pro",
    "version": "0.1.0"
  },
  "tauri": {
    "windows": [
      {
        "title": "TradingAgents Pro",
        "width": 1400,
        "height": 900,
        "minWidth": 1024,
        "minHeight": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "bundle": {
      "active": true,
      "identifier": "com.tradingagents.desktop",
      "targets": ["msi", "nsis"],
      "resources": ["../../run_backend.py"]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    }
  }
}
